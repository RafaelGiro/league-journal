{
  "extends": ["@react-native-community", "prettier"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react-native/no-inline-styles": "off",
    "react-hooks/exhaustive-deps": "warn",
    "@typescript-eslint/no-unused-vars": "warn"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-undef": "off"
      }
    }
  ]
}
